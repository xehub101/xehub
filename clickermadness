local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/LuffyExploits/LuffyExploits/main/Orion%20Library')))()

local Window = OrionLib:MakeWindow({Name = "Clicker Madness by Eno", HidePremium = true, SaveConfig = false, ConfigFolder = "OrionTest"})

local FarmTab = Window:MakeTab({
	Name = "Farming",
	Icon = "",
	PremiumOnly = false
})

local Section = FarmTab:AddSection({
	Name = "Auto Collect"
})
FarmTab:AddToggle({
	Name = "Auto Click",
	Default = false,
	Callback = function(Value)
		AutoClick = Value
		AutoClicker()
	end    
})
FarmTab:AddToggle({
	Name = "Auto Collecter",
	Default = false,
	Callback = function(Value)
		autofarm = Value
		AutoFarm()
	end    
})
FarmTab:AddButton({
	Name = "Collect Chests",
	Callback = function()
      		AutoOpenChests()
  	end    
})
local Section = FarmTab:AddSection({
	Name = "Auto Rebirthing"
})
FarmTab:AddTextbox({
	Name = "Rebirth Amount",
	Default = "Amount Ex: 1000",
	TextDisappear = true,
	Callback = function(Value)
		rebirthamount = Value
	end	  
})
FarmTab:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(Value)
		AutoRebirth = Value
		AutoRebirther()
	end    
})
local Section = FarmTab:AddSection({
	Name = "Bosses"
})
FarmTab:AddToggle({
	Name = "Auto Farm Boss",
	Default = false,
	Callback = function(Value)
		autofarmboss = Value
		AutoBoss()
	end    
})
local BuyTab = Window:MakeTab({
	Name = "Auto Buy",
	Icon = "",
	PremiumOnly = false
})

local Section = BuyTab:AddSection({
	Name = "Auto Buy Upgrades"
})

BuyTab:AddToggle({
	Name = "Auto Upgrades",
	Default = false,
	Callback = function(Value)
		AutoUpgrade = Value
		AutoBuyUpgrades()
	end    
})
BuyTab:AddToggle({
	Name = "Auto Super Upgrades",
	Default = false,
	Callback = function(Value)
		autobuysuperupgrades = Value
		AutoBuySuperUpgrades()
	end    
})

local Section = BuyTab:AddSection({
	Name = "Auto Buy Eggs"
})

BuyTab:AddDropdown({
	Name = "Egg",
	Default = "basic",
	Options = {"basic", "lava", "desert", "ocean", "winter", "toxic", "candy", "forest", "storm", "blocks", "space", "dominus", "infinity", "future", "city", "moon", "fire"},
	Callback = function(Value)
		egg = Value
	end    
})
BuyTab:AddToggle({
	Name = "Auto Buy Egg",
	Default = false,
	Callback = function(Value)
		AutoEgg = Value
		AutoBuyEgg()
	end    
})
